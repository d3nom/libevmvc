{
    "name": "fanjet",
    "displayName": "Fanjet syntax highlighting",
    "description": "Provides syntax higlighting for the Fanjet language",
    "version": "0.1.8",
    "publisher": "libevmvc",
    "engines": {
        "vscode": "^1.32.3"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "c++", "c", "markup"
    ],
    "contributes": {
        "languages": [
            {
                "id": "fanjet",
                "extensions": [
                    ".fan"
                ],
                "aliases": [
                    "Fanjet", "fanjet"
                ],
                "mimetypes": ["text/html"],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "fanjet",
                "scopeName": "text.html.fan",
                "path": "./syntaxes/fanjet.tmLanguage.json",
                "embeddedLanguages": {
                    "section.embedded.source.fan": "html"
                }
            }
        ],
        "snippets": [
            {
                "language": "fanjet",
                "path": "./snippets/fanjet.snippets.json"
            }
        ]
    },
    "scripts": {
        
    }
    
}