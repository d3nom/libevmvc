{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch TESTS",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/bin/libevmvc_tests",
            "args": [
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build/bin/",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        
        {
            "name": "(gdb) Launch web_server",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/bin/evmvc_web_server",
            "args": [
                "1", "8"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build/bin/",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        
        {
            "name": "(gdb) Attach web_server",
            "type": "cppdbg",
            "request": "attach",
            //"preLaunchTask": "launch web_server",
            "processId":"${command:pickProcess}",
            "program": "${workspaceFolder}/build/bin/evmvc_web_server",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        
        {
            "name": "(gdb) Launch fanjet-precompiler",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/bin/fanjet",
            "args": [
                "-vv",
                "-n", "examples",
                "-i", "examples_views.h",
                "-s", "${workspaceFolder}/examples-evmvc/views/",
                "-d", "${workspaceFolder}/build/bin/examples-views/"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build/bin/",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        
    ]
    
}